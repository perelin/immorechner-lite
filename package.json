{
    "name": "immorechner-lite",
    "description": "Immorechner",
    "version": "0.1.0",
    "author": "Sebastian Patino-Lang <patino@p2lab.de>",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "start": "npm run build && node server.js",
        "server_parcel": "NODE_ENV=production node server.js",
        "dev_parcel": "NODE_ENV=development parcel ./public/index.html --out-dir dist_dev",
        "build_parcel": "NODE_ENV=production parcel build ./public/index.html --out-dir dist --detailed-report --no-minify",
        "prebuild": "rm -rf dist",
        "test": "jest --coverage",
        "testwatch": "jest --watchAll --coverage"
    },
    "jest": {
        "collectCoverage": true,
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "transform": {
            ".*\\.(vue)$": "vue-jest",
            "^.+\\.js$": "babel-jest"
        }
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.1.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.0",
        "@fortawesome/free-solid-svg-icons": "^5.1.0",
        "@fortawesome/vue-fontawesome": "^0.1.0",
        "axios": "^0.18.1",
        "bootstrap": "^4.3.1",
        "bootstrap-vue": "^2.0.2",
        "chart.js": "^2.7.2",
        "cssnano": "^4.0.3",
        "express": "^4.16.3",
        "i": "^0.3.6",
        "npm": "^6.11.3",
        "numeral": "^2.0.6",
        "path": "^0.12.7",
        "serve-static": "^1.13.2",
        "vue": "^2.5.11",
        "vue-chartjs": "^3.3.2",
        "vue-hot-reload-api": "^2.3.0",
        "vue-numeric": "^2.3.0",
        "vue-scrollto": "^2.11.0",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/component-compiler-utils": "^2.1.0",
        "@vue/test-utils": "^1.0.0-beta.24",
        "@vue/cli-service": "^3.11.0",
        "babel-core": "^6.26.3",
        "babel-jest": "^23.4.2",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-3": "^6.24.1",
        "css-loader": "^0.28.7",
        "jest": "^23.5.0",
        "parcel-bundler": "^1.12.3",
        "vue-jest": "^2.6.0",
        "vue-loader": "^15.2.4",
        "vue-template-compiler": "^2.5.16",
        "vue-test-utils": "^1.0.0-beta.11"
    }
}